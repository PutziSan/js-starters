{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "rules": {
    "import/prefer-default-export": 0,
    "function-paren-newline": ["error", "consistent"],
    "quotes": ["error", "single"],
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "trailingComma": "es5"
    }]
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
